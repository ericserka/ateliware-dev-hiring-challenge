generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           String       @id
  repositories Repository[]
}

model Repository {
  id      Int    @id @default(autoincrement())
  dataObj Json
  User    User   @relation(fields: [userId], references: [id])
  userId  String
}
